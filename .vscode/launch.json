  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "oas-express-starter",
      "program": "${workspaceFolder}/dist/server.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "CONFIG_MONGO_URI": "mongodb://root:password@127.0.0.1:27017?authSource=admin",
        "CONFIG_DB_NAME": "testDB",
        "DEBUG": "*oas-express-starter*",
      }
    }
  ]
}